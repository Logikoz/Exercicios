#include <stdlib.h>
#include <stdio.h>
#include <math.h>

#define Max 3

double RaizVetor(float V[Max]){
	int i;
	double somaPotencia = 0;
	for(i = 0; i < Max; i++){
		somaPotencia += pow(V[i], 2.0);
	}
	return sqrt(somaPotencia);
}

void main(){
	float X[Max] = { 1, 2, 3};
	float Y[Max] = {-4, 0, 5};
	
	double raizX = RaizVetor(X);
	double raizY = RaizVetor(Y);
	
	printf("%lf", raizX * raizY * sin(180));
	printf("\n%f",sin(180));
}

